    "deploy": "maint/deploy.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-ex-assign": 0,
      "eqeqeq": 0,
      "no-throw-literal": 0,
      "default-case": 0,
      "no-self-assign": 0,
      "no-mixed-operators": 0,
      "no-new-func": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
